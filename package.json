{
  "name": "ReactNativeStarterkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "ENVFILE=.env.dev react-native run-android --mode=DevelopmentDebug",
    "android:staging": "ENVFILE=.env.staging react-native run-android  --mode=StagingDebug",
    "android:prod": "ENVFILE=.env.prod react-native run-android  --mode=ProductionDebug",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --scheme 'ReactNativeStarterkit-Development'",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios --scheme 'ReactNativeStarterkit-Staging'",
    "ios:prod": "ENVFILE=.env.prod react-native run-ios --scheme 'ReactNativeStarterkit'",
    "lint": "eslint .",
    "start": "react-native start",
    "test:unit": "jest",
    "test:e2e:android:dev": "detox build --configuration android.emu.dev.debug && detox test --configuration android.emu.dev.debug",
    "test:e2e:android:staging": "detox build --configuration android.emu.staging.debug && detox test --configuration android.emu.staging.debug",
    "test:e2e:android:prod": "detox build --configuration android.emu.prod.debug && detox test --configuration android.emu.prod.debug",
    "test:e2e:ios:dev": "detox build --configuration ios.sim.dev.debug && detox test --configuration ios.sim.dev.debug",
    "test:e2e:ios:staging": "detox build --configuration ios.sim.staging.debug && detox test --configuration ios.sim.staging.debug",
    "test:e2e:ios:prod": "detox build --configuration ios.sim.prod.debug && detox test --configuration ios.sim.prod.debug",
    "postinstall": "bash etc/postinstall.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@react-native-community/blur": "^4.3.2",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/crashlytics": "^18.3.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@storybook/addon-actions": "^7.3.0",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-react-native-web": "^0.0.21",
    "@storybook/blocks": "^7.2.3",
    "@storybook/react": "^7.2.3",
    "@storybook/react-webpack5": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.19.7",
    "detox": "^20.11.2",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-tsdoc-require": "^0.0.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.4.1",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.7",
    "react-test-renderer": "18.2.0",
    "storybook": "^7.2.3",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  }
}
